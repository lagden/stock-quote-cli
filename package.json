{
  "name": "lagden-stock-quote-cli",
  "version": "1.1.0",
  "description": "Cotações da Bolsa de Valores",
  "keywords": [
    "cli-app",
    "cli",
    "stock",
    "quote",
    "search"
  ],
  "bug": {
    "url": "https://github.com/lagden/stock-quote/issues",
    "email": "lagden@gmail.com"
  },
  "license": "MIT",
  "author": {
    "name": "Thiago Lagden",
    "email": "lagden@gmail.com"
  },
  "files": [
    "cli.js",
    "README.md",
    "license",
    "man"
  ],
  "preferGlobal": true,
  "bin": {
    "cotacao": "./cli.js"
  },
  "man": "./man/cotacao.1",
  "repository": "lagden/stock-quote-cli",
  "scripts": {
    "man": "mkdir -p man && remark doc/*.?.md",
    "test": "ava"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "devDependencies": {
    "ava": "3.13.0",
    "remark-cli": "9.0.0",
    "remark-man": "7.0.0",
    "xo": "0.34.2"
  },
  "dependencies": {
    "chalk": "4.1.0",
    "cli-table2": "0.2.0",
    "commander": "6.2.0",
    "lagden-stock-quote": "1.2.1"
  },
  "xo": {
    "envs": [
      "es6"
    ],
    "semicolon": false
  },
  "remarkConfig": {
    "output": "man/cotacao.1",
    "plugins": {
      "remark-man": {
        "name": "cotacao",
        "date": "2017-01-25",
        "manual": "cotacao manual",
        "version": "1.0.0"
      }
    }
  }
}